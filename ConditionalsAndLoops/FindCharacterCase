package ConditionalAndLoops;
import java.util.Scanner;
public class FindCharacterCase {
    public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    char c = sc.next().charAt(0);
    if(c >= 'a' && c <= 'z'){
        System.out.print("0");
    }else if(c >= 'A' && c <='Z'){
        System.out.print("1");
    }else{
        System.out.print(-1);
    }
    }
}
